{
  "name": "fortune-coookie-jonas",
  "version": "1.0.3",
  "description": "Maccyber will tell your fortune",
  "main": "index.js",
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "npm run build && next start",
    "deploy": "npm test && now --target production",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "author": "Geir GÃ¥sodden <geir.gasodden@pythonia.no> (https://github.com/zrrrzzt)",
  "license": "MIT",
  "dependencies": {
    "fortune-cookie": "0.0.1",
    "knuth-shuffle": "1.0.8",
    "next": "9.1.5",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "nyc": "14.1.1",
    "standard": "14.3.1"
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "standard": {
    "ignore": [
      "/static/*.js"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  }
}
